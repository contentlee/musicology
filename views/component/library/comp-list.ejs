  <div class="container">
    <div class="category">
      <div class="category-wrapper">
        <a href="/library" class="all" data-name="ALL">ALL</a>
        <a href="/library/GENERAL" data-name="GENERAL">GENERAL MUSICOLOGY</a>
        <a href="/library/HISTORY" data-name="HISTORY">HISTORY AND BIOGRAPHY</a>
        <a href="/library/CULTURE" data-name="CULTURE">MUSIC AND CULTURE</a>
        <a href="/library/THEORY" data-name="THEORY">MUSIC THEORY</a>
        <a href="/library/ESSENTIAL" data-name="ESSENTIAL">ESSENTIAL REFERENCES</a>
      </div>
    </div>

    <div class="library-wrapper">
      <% if(books.length) { %>
      <!--  -->
      <% for(var i=0; i<books.length; i++){ %>
      <div class="book-wrapper">
        <div class="book-img">
          <div class="img">
            <img src="<%= books[i].이미지 %>" alt="">
          </div>
        </div>
        <a href="/detail/<%= books[i]._id%>" class="book-info">
          <span class="title"><%= books[i].제목 %></span>
          <span class="subtitle"><%= books[i].부제목 %></span>
          <span class="author"><%= books[i].작가 %></span>
          <span class="date"><%= books[i].연도 %></span>
          <span class="publish"><%= books[i].출판사 %></span>
        </a>
        <button class="edit" onclick="location.href = '/edit/<%= books[i]._id %>'">수정</button>
        <button class="delete" data-id="<%= books[i]._id %>">삭제</button>


      </div>
      <% } %>
      <%} %>
    </div>
  </div>